{
  "manifest_version": 2,
  "name": "Ask Jeeves, Again...Again",
  "description": "Jeeves retired back in 2006, but, now, he is back. Get your search results from the oldest player in the game, eventually... ",
  "version": "1.0",
  "minimum_chrome_version": "20",
  // "content_security_policy": "script-src 'self' 'sha256-WOdSzz11/3cpqOdrm89LBL2UPwEU9EhbDtMy2OciEhs='",
  "permissions": [
  "tabs", "activeTab"],
